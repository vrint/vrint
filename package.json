{
  "name": "vrint",
  "version": "1.0.0",
  "description": "A UI tookit based on Vuejs",
  "author": "chenze2168@gmail.cpom",
  "private": true,
  "scripts": {
    "dev": "cross-env NODE_ENV=development webpack-dev-server --config build/webpack.dev.config.js --open --hot",
    "start": "npm run dev",
    "lint": "eslint --ext .js,.vue src --fix",
    "test": "cross-env NODE_ENV=test jest -i",
    "build": "cross-env NODE_ENV=production webpack --config build/config.js --progress --hide-modules"
  },
  "dependencies": {},
  "devDependencies": {
    "autoprefixer": "7.2.5",
    "avoriaz": "6.3.0",
    "babel-cli": "6.26.0",
    "babel-core": "6.26.0",
    "babel-eslint": "^8.2.2",
    "babel-jest": "22.1.0",
    "babel-loader": "^7.1.4",
    "babel-plugin-add-filehash": "6.9.5",
    "babel-plugin-detective": "2.0.0",
    "babel-plugin-module-resolver": "3.0.0",
    "babel-plugin-transform-async-to-generator": "6.24.1",
    "babel-plugin-transform-define": "^1.3.0",
    "babel-plugin-transform-runtime": "6.23.0",
    "babel-polyfill": "6.26.0",
    "babel-preset-env": "1.6.1",
    "babel-preset-es2015": "6.24.1",
    "babel-preset-stage-2": "6.24.1",
    "cross-env": "^5.1.4",
    "cross-spawn": "5.1.0",
    "css-loader": "^0.28.10",
    "css-mqpacker": "^6.0.2",
    "cssnano": "3.10.0",
    "dotenv": "4.0.0",
    "eslint": "4.15.0",
    "eslint-config-standard": "^11.0.0-beta.0",
    "eslint-friendly-formatter": "3.0.0",
    "eslint-loader": "1.9.0",
    "eslint-plugin-html": "^4.0.2",
    "eslint-plugin-import": "^2.8.0",
    "eslint-plugin-local-rules": "^0.1.0",
    "eslint-plugin-node": "^5.2.1",
    "eslint-plugin-promise": "^3.6.0",
    "eslint-plugin-standard": "^3.0.1",
    "eventsource-polyfill": "0.9.6",
    "extract-text-webpack-plugin": "3.0.2",
    "file-loader": "^1.1.11",
    "friendly-errors-webpack-plugin": "1.6.1",
    "function-bind": "1.1.1",
    "husky": "0.14.3",
    "inquirer": "5.0.0",
    "jest": "21.2.1",
    "jest-cli": "21.2.1",
    "jest-css-modules": "1.1.0",
    "jest-serializer-html": "5.0.0",
    "jest-vue-preprocessor": "1.3.1",
    "optimize-css-assets-webpack-plugin": "3.2.0",
    "optimize-js-plugin": "0.0.4",
    "postcss-loader": "2.0.10",
    "progress-bar-webpack-plugin": "1.10.0",
    "pug": "2.0.0-rc.4",
    "pug-loader": "2.3.0",
    "ress": "1.2.2",
    "rimraf": "2.6.2",
    "semver": "^5.5.0",
    "serialize-javascript": "1.4.0",
    "shelljs": "^0.8.1",
    "style-loader": "0.19.1",
    "stylus": "0.54.5",
    "stylus-loader": "^3.0.2",
    "uglifyjs-webpack-plugin": "1.1.6",
    "url-loader": "^1.0.1",
    "vue": "^2.5.16",
    "vue-loader": "^14.2.1",
    "vue-router": "3.0.1",
    "vue-server-renderer": "^2.5.14",
    "vue-template-compiler": "^2.5.14",
    "webpack": "3.10.0",
    "webpack-bundle-analyzer": "2.9.2",
    "webpack-bundle-size-analyzer": "2.7.0",
    "webpack-dev-server": "^2.11.1",
    "webpack-merge": "^4.1.2",
    "write-file-webpack-plugin": "4.2.0"
  },
  "engines": {
    "node": ">= 4.0.0",
    "npm": ">= 3.0.0"
  },
  "jest": {
    "verbose": false,
    "roots": [
      "<rootDir>/src"
    ],
    "moduleFileExtensions": [
      "js",
      "vue"
    ],
    "moduleDirectories": [
      "node_modules"
    ],
    "moduleNameMapper": {
      "^@(.*)$": "<rootDir>/src/$1"
    },
    "transform": {
      ".*\\.(vue)$": "<rootDir>/node_modules/jest-vue-preprocessor",
      ".*\\.(vue|js)$": "<rootDir>/node_modules/babel-jest"
    },
    "transformIgnorePatterns": [
      "node_modules/(?!vue-router)"
    ],
    "snapshotSerializers": [
      "jest-serializer-html"
    ]
  },
  "browserslist": [
    "> 1%",
    "last 2 versions",
    "not ie <= 8"
  ]
}
